<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.ecloud.oauthz.dao.IOauthAppInfoDao">
  <sql id="Base_Column_List">
    app_code, app_name, app_key, app_secret, redirect_url, image_url, create_time
  </sql>
  <select id="selectByAppkey" parameterType="java.lang.String" resultType="com.cloud.ecloud.oauthz.entity.OauthAppInfo">
    select 
    <include refid="Base_Column_List" />
    from oauth_app_info
    where app_key = #{appKey,jdbcType=VARCHAR}
  </select>
</mapper>